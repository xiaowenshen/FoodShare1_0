                                                <div class="inside">
                                                    <h5><a href="#"><%#Eval("CTitle") %></a></h5>
                                                    <p><%#Eval("CIntroduce") %></p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                      
                            </ItemTemplate>
                                
                            <FooterTemplate>
                                <br />
                                <div class="page">
                                     <p><%=CookPageBar%></p>
                                </div>
                            </FooterTemplate>
                        </asp:Repeater>
                            </div>
                    </div>
                        </form>
