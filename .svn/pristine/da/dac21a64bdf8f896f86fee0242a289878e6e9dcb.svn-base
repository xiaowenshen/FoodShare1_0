                    </div>
                        <div class="row projects-holder">
                        <asp:Repeater ID="Repeater2" runat="server">
                            
                                     <ItemTemplate>
                                
                                    <div class="col-md-4 col-sm-6">
                                        <div class="project-item">
                                            <img src="<%#Eval("path") %>" alt=""/>
